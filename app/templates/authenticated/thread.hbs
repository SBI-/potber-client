{{! @glint-nocheck }}{{#unless @model.threadResource.isError}}
  {{page-title this.pageTitle}}

  <Nav::Routes::Thread
    @threadId={{@model.threadId}}
    @threadResource={{@model.threadResource}}
  />

  <Routes::Thread::Page
    @threadResource={{@model.threadResource}}
    @lastReadPost={{@model.lastReadPost}}
  />
{{else}}
  {{page-title '404'}}
  <Misc::Error::NotFound />
{{/unless}}